    function checkIE(userAgent) {
        userAgent = userAgent && userAgent.toLowerCase ? userAgent.toLowerCase() : '';

        function isBrowser(browser) {
            browser = browser.toLowerCase();
            return new RegExp(browser).test(userAgent);
